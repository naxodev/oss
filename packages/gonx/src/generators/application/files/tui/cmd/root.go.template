package cmd

import (
	"fmt"
	"os"

	tea "github.com/charmbracelet/bubbletea"
	"<%= moduleName %>/ui"
)

// Execute starts the TUI application
func Execute() {
	m := ui.NewModel()
	p := tea.NewProgram(m, tea.WithAltScreen())
	
	if _, err := p.Run(); err != nil {
		fmt.Printf("Error running <%= projectName %>: %v\n", err)
		os.Exit(1)
	}
}
